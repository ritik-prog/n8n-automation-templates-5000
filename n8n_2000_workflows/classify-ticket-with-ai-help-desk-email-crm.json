{
  "name": "Classify ticket with ai from Help desk email and send to CRM",
  "nodes": [
    {
      "id": "trigger-help-desk-email",
      "name": "Incoming Trigger",
      "type": "n8n-nodes-base.webhook",
      "typeVersion": 1,
      "position": [
        -600,
        87500
      ],
      "parameters": {
        "httpMethod": "POST",
        "path": "classify-ticket-with-ai-from-help-desk-email-and-send-to-crm",
        "responseCode": 200,
        "responseMode": "onReceived"
      }
    },
    {
      "id": "fetch-help-desk-email",
      "name": "Fetch data from Help desk email",
      "type": "n8n-nodes-base.httpRequest",
      "typeVersion": 1,
      "position": [
        -300,
        87500
      ],
      "parameters": {
        "method": "GET",
        "url": "https://api.example.com/help-desk-email",
        "responseFormat": "json"
      }
    },
    {
      "id": "ai-classify-ticket-with-ai",
      "name": "AI: Classify Ticket With Ai",
      "type": "n8n-nodes-base.openAIApi",
      "typeVersion": 1,
      "position": [
        0,
        87500
      ],
      "parameters": {
        "model": "gpt-4",
        "operation": "complete",
        "prompt": "Perform the following action on the incoming data: classify ticket with AI"
      }
    },
    {
      "id": "send-crm",
      "name": "Send to CRM",
      "type": "n8n-nodes-base.httpRequest",
      "typeVersion": 1,
      "position": [
        300,
        87500
      ],
      "parameters": {
        "method": "POST",
        "url": "https://api.example.com/crm",
        "responseFormat": "json",
        "options": {}
      }
    },
    {
      "id": "error-handler-600-87500",
      "name": "Send Error to Slack",
      "type": "n8n-nodes-base.slack",
      "typeVersion": 1,
      "position": [
        600,
        87500
      ],
      "parameters": {
        "resource": "message",
        "operation": "post",
        "channel": "#automation-alerts",
        "text": "={{ $json.get('error_message', 'An error occurred') }}"
      }
    }
  ],
  "connections": {
    "Incoming Trigger": {
      "main": [
        [
          {
            "node": "Fetch data from Help desk email",
            "type": "main",
            "index": 0
          }
        ]
      ]
    },
    "Fetch data from Help desk email": {
      "main": [
        {
          "node": "AI: Classify Ticket With Ai",
          "type": "main",
          "index": 0
        }
      ]
    },
    "AI: Classify Ticket With Ai": {
      "main": [
        [
          {
            "node": "AI: Classify Ticket With Ai",
            "type": "main",
            "index": 0
          }
        ]
      ]
    },
    "Send to CRM": {
      "main": [
        [
          {
            "node": "Send Error to Slack",
            "type": "main",
            "index": 0
          }
        ]
      ]
    }
  },
  "settings": {},
  "active": false
}