{
  "name": "Score lead with ai from Facebook Ads and send to Gmail",
  "nodes": [
    {
      "id": "trigger-facebook-ads",
      "name": "Incoming Trigger",
      "type": "n8n-nodes-base.webhook",
      "typeVersion": 1,
      "position": [
        -600,
        32000
      ],
      "parameters": {
        "httpMethod": "POST",
        "path": "score-lead-with-ai-from-facebook-ads-and-send-to-gmail",
        "responseCode": 200,
        "responseMode": "onReceived"
      }
    },
    {
      "id": "fetch-facebook-ads",
      "name": "Fetch data from Facebook Ads",
      "type": "n8n-nodes-base.httpRequest",
      "typeVersion": 1,
      "position": [
        -300,
        32000
      ],
      "parameters": {
        "method": "GET",
        "url": "https://api.example.com/facebook-ads",
        "responseFormat": "json"
      }
    },
    {
      "id": "ai-score-lead-with-ai",
      "name": "AI: Score Lead With Ai",
      "type": "n8n-nodes-base.openAIApi",
      "typeVersion": 1,
      "position": [
        0,
        32000
      ],
      "parameters": {
        "model": "gpt-4",
        "operation": "complete",
        "prompt": "Perform the following action on the incoming data: score lead with AI"
      }
    },
    {
      "id": "send-gmail",
      "name": "Send to Gmail",
      "type": "n8n-nodes-base.httpRequest",
      "typeVersion": 1,
      "position": [
        300,
        32000
      ],
      "parameters": {
        "method": "POST",
        "url": "https://api.example.com/gmail",
        "responseFormat": "json",
        "options": {}
      }
    },
    {
      "id": "error-handler-600-32000",
      "name": "Send Error to Slack",
      "type": "n8n-nodes-base.slack",
      "typeVersion": 1,
      "position": [
        600,
        32000
      ],
      "parameters": {
        "resource": "message",
        "operation": "post",
        "channel": "#automation-alerts",
        "text": "={{ $json.get('error_message', 'An error occurred') }}"
      }
    }
  ],
  "connections": {
    "Incoming Trigger": {
      "main": [
        [
          {
            "node": "Fetch data from Facebook Ads",
            "type": "main",
            "index": 0
          }
        ]
      ]
    },
    "Fetch data from Facebook Ads": {
      "main": [
        {
          "node": "AI: Score Lead With Ai",
          "type": "main",
          "index": 0
        }
      ]
    },
    "AI: Score Lead With Ai": {
      "main": [
        [
          {
            "node": "AI: Score Lead With Ai",
            "type": "main",
            "index": 0
          }
        ]
      ]
    },
    "Send to Gmail": {
      "main": [
        [
          {
            "node": "Send Error to Slack",
            "type": "main",
            "index": 0
          }
        ]
      ]
    }
  },
  "settings": {},
  "active": false
}