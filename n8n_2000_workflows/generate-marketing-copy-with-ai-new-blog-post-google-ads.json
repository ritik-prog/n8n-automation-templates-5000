{
  "name": "Generate marketing copy with ai from New blog post and send to Google Ads",
  "nodes": [
    {
      "id": "trigger-new-blog-post",
      "name": "Incoming Trigger",
      "type": "n8n-nodes-base.webhook",
      "typeVersion": 1,
      "position": [
        -600,
        61300
      ],
      "parameters": {
        "httpMethod": "POST",
        "path": "generate-marketing-copy-with-ai-from-new-blog-post-and-send-to-google-ads",
        "responseCode": 200,
        "responseMode": "onReceived"
      }
    },
    {
      "id": "fetch-new-blog-post",
      "name": "Fetch data from New blog post",
      "type": "n8n-nodes-base.httpRequest",
      "typeVersion": 1,
      "position": [
        -300,
        61300
      ],
      "parameters": {
        "method": "GET",
        "url": "https://api.example.com/new-blog-post",
        "responseFormat": "json"
      }
    },
    {
      "id": "ai-generate-marketing-copy-with-ai",
      "name": "AI: Generate Marketing Copy With Ai",
      "type": "n8n-nodes-base.openAIApi",
      "typeVersion": 1,
      "position": [
        0,
        61300
      ],
      "parameters": {
        "model": "gpt-4",
        "operation": "complete",
        "prompt": "Perform the following action on the incoming data: generate marketing copy with AI"
      }
    },
    {
      "id": "send-google-ads",
      "name": "Send to Google Ads",
      "type": "n8n-nodes-base.httpRequest",
      "typeVersion": 1,
      "position": [
        300,
        61300
      ],
      "parameters": {
        "method": "POST",
        "url": "https://api.example.com/google-ads",
        "responseFormat": "json",
        "options": {}
      }
    },
    {
      "id": "error-handler-600-61300",
      "name": "Send Error to Slack",
      "type": "n8n-nodes-base.slack",
      "typeVersion": 1,
      "position": [
        600,
        61300
      ],
      "parameters": {
        "resource": "message",
        "operation": "post",
        "channel": "#automation-alerts",
        "text": "={{ $json.get('error_message', 'An error occurred') }}"
      }
    }
  ],
  "connections": {
    "Incoming Trigger": {
      "main": [
        [
          {
            "node": "Fetch data from New blog post",
            "type": "main",
            "index": 0
          }
        ]
      ]
    },
    "Fetch data from New blog post": {
      "main": [
        {
          "node": "AI: Generate Marketing Copy With Ai",
          "type": "main",
          "index": 0
        }
      ]
    },
    "AI: Generate Marketing Copy With Ai": {
      "main": [
        [
          {
            "node": "AI: Generate Marketing Copy With Ai",
            "type": "main",
            "index": 0
          }
        ]
      ]
    },
    "Send to Google Ads": {
      "main": [
        [
          {
            "node": "Send Error to Slack",
            "type": "main",
            "index": 0
          }
        ]
      ]
    }
  },
  "settings": {},
  "active": false
}