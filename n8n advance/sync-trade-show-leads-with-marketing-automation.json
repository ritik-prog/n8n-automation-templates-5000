{
  "name": "sync-trade-show-leads-with-marketing-automation",
  "nodes": [
    {
      "parameters": {
        "httpMethod": "POST",
        "path": "tradeshow-leads"
      },
      "id": "ts1",
      "name": "Tradeshow Webhook",
      "type": "n8n-nodes-base.webhook",
      "typeVersion": 1,
      "position": [
        100,
        320
      ]
    },
    {
      "parameters": {
        "functionCode": "const b=$json.body||$json;const leads=(b.leads||[]).map(l=>({json:{email:(l.email||'').toLowerCase(),first:l.first||'',last:l.last||'',company:l.company||'',title:l.title||'',notes:l.notes||'',event:b.event||'',source:'tradeshow'}}));return leads;"
      },
      "id": "ts2",
      "name": "Normalize Leads",
      "type": "n8n-nodes-base.function",
      "typeVersion": 1,
      "position": [
        320,
        320
      ]
    },
    {
      "parameters": {
        "url": "https://person.clearbit.com/v2/combined/find?email={{encodeURIComponent($json.email)}}",
        "authentication": "none",
        "options": {
          "headersUi": {
            "parameter": [
              {
                "name": "Authorization",
                "value": "Bearer {{$credentials.clearbitKey}}"
              }
            ]
          }
        }
      },
      "id": "ts3",
      "name": "Enrich (Clearbit placeholder)",
      "type": "n8n-nodes-base.httpRequest",
      "typeVersion": 1,
      "position": [
        540,
        280
      ]
    },
    {
      "parameters": {
        "operation": "create",
        "resource": "contact",
        "additionalFields": {
          "propertiesUi": {
            "property": [
              {
                "name": "email",
                "value": "={{$json.email}}"
              },
              {
                "name": "firstname",
                "value": "={{$prevNode['Normalize Leads'].json.first}}"
              },
              {
                "name": "lastname",
                "value": "={{$prevNode['Normalize Leads'].json.last}}"
              },
              {
                "name": "company",
                "value": "={{$prevNode['Normalize Leads'].json.company}}"
              },
              {
                "name": "jobtitle",
                "value": "={{$prevNode['Normalize Leads'].json.title}}"
              },
              {
                "name": "lead_source",
                "value": "={{$prevNode['Normalize Leads'].json.source}}"
              },
              {
                "name": "last_event",
                "value": "={{$prevNode['Normalize Leads'].json.event}}"
              }
            ]
          }
        }
      },
      "id": "ts4",
      "name": "Upsert to HubSpot",
      "type": "n8n-nodes-base.hubspot",
      "typeVersion": 1,
      "position": [
        540,
        360
      ],
      "credentials": {
        "hubspotApi": {
          "id": "hubspot_api_cred",
          "name": "HubSpot API"
        }
      }
    },
    {
      "parameters": {
        "resource": "message",
        "operation": "send",
        "fromEmail": "events@yourcompany.com",
        "toEmail": "={{$json.email}}",
        "subject": "Great meeting you at {{$prevNode['Normalize Leads'].json.event}}",
        "text": "Hi {{$prevNode['Normalize Leads'].json.first}},\\nThanks for stopping by our booth at {{$prevNode['Normalize Leads'].json.event}}. Here are resources: https://example.com/demo\\n\u2014 Team"
      },
      "id": "ts5",
      "name": "Send Follow-up Email",
      "type": "n8n-nodes-base.smtp",
      "typeVersion": 1,
      "position": [
        760,
        320
      ],
      "credentials": {
        "smtp": {
          "id": "smtp_cred",
          "name": "SMTP"
        }
      }
    },
    {
      "parameters": {
        "channel": "#events",
        "text": "Synced tradeshow lead {{$json.email}} ({{$prevNode['Normalize Leads'].json.event}}) to HubSpot and sent follow-up."
      },
      "id": "ts6",
      "name": "Slack Notify",
      "type": "n8n-nodes-base.slack",
      "typeVersion": 1,
      "position": [
        980,
        320
      ]
    }
  ],
  "connections": {
    "Tradeshow Webhook": {
      "main": [
        [
          {
            "node": "Normalize Leads",
            "type": "main",
            "index": 0
          }
        ]
      ]
    },
    "Normalize Leads": {
      "main": [
        [
          {
            "node": "Enrich (Clearbit placeholder)",
            "type": "main",
            "index": 0
          },
          {
            "node": "Upsert to HubSpot",
            "type": "main",
            "index": 0
          },
          {
            "node": "Send Follow-up Email",
            "type": "main",
            "index": 0
          },
          {
            "node": "Slack Notify",
            "type": "main",
            "index": 0
          }
        ]
      ]
    }
  },
  "active": false
}