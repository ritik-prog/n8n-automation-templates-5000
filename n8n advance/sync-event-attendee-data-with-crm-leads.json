{
  "name": "sync-event-attendee-data-with-crm-leads",
  "nodes": [
    {
      "parameters": {
        "httpMethod": "POST",
        "path": "event-attendee"
      },
      "id": "ev1",
      "name": "Event Webhook (Eventbrite/Zoom)",
      "type": "n8n-nodes-base.webhook",
      "typeVersion": 1,
      "position": [
        100,
        300
      ]
    },
    {
      "parameters": {
        "functionCode": "const b=$json.body||$json;const list=(b.attendees||[]).map(a=>({json:a}));return list;"
      },
      "id": "ev2",
      "name": "Fan-out Attendees",
      "type": "n8n-nodes-base.function",
      "typeVersion": 1,
      "position": [
        320,
        300
      ]
    },
    {
      "parameters": {
        "operation": "search",
        "resource": "contact",
        "additionalFields": {
          "email": "={{$json.email}}"
        }
      },
      "id": "ev3",
      "name": "Find Contact",
      "type": "n8n-nodes-base.hubspot",
      "typeVersion": 1,
      "position": [
        540,
        300
      ],
      "credentials": {
        "hubspotApi": {
          "id": "hubspot_api_cred",
          "name": "HubSpot API"
        }
      }
    },
    {
      "parameters": {
        "resource": "contact",
        "operation": "upsert",
        "additionalFields": {
          "propertiesUi": {
            "propertyValues": [
              {
                "property": "email",
                "value": "={{$json.email}}"
              },
              {
                "property": "firstname",
                "value": "={{$json.firstName}}"
              },
              {
                "property": "lastname",
                "value": "={{$json.lastName}}"
              },
              {
                "property": "company",
                "value": "={{$json.company}}"
              },
              {
                "property": "jobtitle",
                "value": "={{$json.title}}"
              },
              {
                "property": "last_event_attended",
                "value": "={{$json.eventId || ''}}"
              }
            ]
          }
        }
      },
      "id": "ev4",
      "name": "Upsert Contact",
      "type": "n8n-nodes-base.hubspot",
      "typeVersion": 1,
      "position": [
        760,
        300
      ],
      "credentials": {
        "hubspotApi": {
          "id": "hubspot_api_cred",
          "name": "HubSpot API"
        }
      }
    },
    {
      "parameters": {
        "resource": "note",
        "operation": "create",
        "additionalFields": {
          "propertiesUi": {
            "propertyValues": [
              {
                "property": "hs_note_body",
                "value": "={{'Attended event: '+($json.eventName||'')+' | Checked-in: '+($json.checkin?'Yes':'No')}}"
              },
              {
                "property": "hs_timestamp",
                "value": "={{$now}}"
              }
            ]
          }
        }
      },
      "id": "ev5",
      "name": "Add Note",
      "type": "n8n-nodes-base.hubspot",
      "typeVersion": 1,
      "position": [
        980,
        300
      ],
      "credentials": {
        "hubspotApi": {
          "id": "hubspot_api_cred",
          "name": "HubSpot API"
        }
      }
    }
  ],
  "connections": {
    "Event Webhook (Eventbrite/Zoom)": {
      "main": [
        [
          {
            "node": "Fan-out Attendees",
            "type": "main",
            "index": 0
          }
        ]
      ]
    },
    "Fan-out Attendees": {
      "main": [
        [
          {
            "node": "Find Contact",
            "type": "main",
            "index": 0
          }
        ]
      ]
    },
    "Find Contact": {
      "main": [
        [
          {
            "node": "Upsert Contact",
            "type": "main",
            "index": 0
          }
        ]
      ]
    },
    "Upsert Contact": {
      "main": [
        [
          {
            "node": "Add Note",
            "type": "main",
            "index": 0
          }
        ]
      ]
    }
  },
  "active": false
}